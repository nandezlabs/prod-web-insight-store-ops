{
  "version": 2,
  "builds": [
    {
      "src": "auth-login.ts",
      "use": "@vercel/node"
    },
    {
      "src": "auth-logout.ts",
      "use": "@vercel/node"
    },
    {
      "src": "auth-validate.ts",
      "use": "@vercel/node"
    },
    {
      "src": "store-checklist.ts",
      "use": "@vercel/node"
    },
    {
      "src": "store-inventory.ts",
      "use": "@vercel/node"
    },
    {
      "src": "store-replacement.ts",
      "use": "@vercel/node"
    },
    {
      "src": "admin-dashboard.ts",
      "use": "@vercel/node"
    },
    {
      "src": "admin-forms.ts",
      "use": "@vercel/node"
    },
    {
      "src": "admin-force-logout.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "/auth-login.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/auth/logout",
      "dest": "/auth-logout.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/auth/validate",
      "dest": "/auth-validate.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/store/checklist",
      "dest": "/store-checklist.ts",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/api/store/inventory",
      "dest": "/store-inventory.ts",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/store/replacement",
      "dest": "/store-replacement.ts",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/admin/dashboard",
      "dest": "/admin-dashboard.ts",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/admin/forms",
      "dest": "/admin-forms.ts",
      "methods": ["GET", "POST", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/admin/force-logout",
      "dest": "/admin-force-logout.ts",
      "methods": ["POST", "OPTIONS"]
    }
  ],
  "env": {
    "NOTION_API_KEY": "@notion-api-key",
    "DB_STORE_USERS": "@db-store-users",
    "DB_CHECKLISTS": "@db-checklists",
    "DB_INVENTORY": "@db-inventory",
    "DB_REPLACEMENTS": "@db-replacements",
    "DB_FORMS": "@db-forms",
    "DB_ANALYTICS_LOGS": "@db-analytics-logs",
    "JWT_SECRET": "@jwt-secret",
    "ENCRYPTION_KEY": "@encryption-key",
    "ALLOWED_ORIGINS": "@allowed-origins"
  }
}
